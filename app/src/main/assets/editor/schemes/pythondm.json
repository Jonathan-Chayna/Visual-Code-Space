{
  "name": "Python Dark Theme",
  "tokenColors": [
    {
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#737185b1"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#00FF7F"
      }
    },
    {
      "name": "Regexp",
      "scope": "string.regexp",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#FF4500"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.language",
        "variable.other"
      ],
      "settings": {
        "foreground": "#1E90FF"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#C0C0C0"
      }
    },
    {
      "name": "Class name",
      "scope": [
        "entity.name.class",
        "entity.name.type",
        "entity.name.namespace",
        "entity.name.scope-resolution"
      ],
      "settings": {
        "fontStyle": "",
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Function name",
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#1E90FF"
      }
    },
    {
      "name": "Variable start",
      "scope": "punctuation.definition.variable",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Embedded code markers",
      "scope": [
        "punctuation.section.embedded.begin",
        "punctuation.section.embedded.end"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Built-in constant",
      "scope": [
        "constant.language",
        "meta.preprocessor"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Support.construct",
      "scope": [
        "support.function.construct",
        "keyword.other.new"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "User-defined constant",
      "scope": [
        "constant.character",
        "constant.other"
      ],
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Inherited class",
      "scope": "entity.other.inherited-class",
      "settings": {
        "foreground": "#6A5ACD"
      }
    },
    {
      "name": "Function argument",
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "name": "Tag name",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#1E90FF"
      }
    },
    {
      "name": "Tag start/end",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#666666"
      }
    },
    {
      "name": "Tag attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#C0C0C0"
      }
    },
    {
      "name": "Library function",
      "scope": "support.function",
      "settings": {
        "foreground": "#1E90FF"
      }
    },
    {
      "name": "Continuation",
      "scope": "punctuation.separator.continuation",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Library constant",
      "scope": [
        "support.constant",
        "support.variable"
      ],
      "settings": {}
    },
    {
      "name": "Library class/type",
      "scope": [
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Library Exception",
      "scope": "support.type.exception",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "Library variable",
      "scope": "support.other.variable",
      "settings": {}
    },
    {
      "name": "Invalid",
      "scope": "invalid",
      "settings": {
        "foreground": "#FF6347"
      }
    },
    {
      "name": "diff: header",
      "scope": [
        "meta.diff",
        "meta.diff.header"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#1E90FF"
      }
    },
    {
      "name": "diff: deleted",
      "scope": "markup.deleted",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF6347"
      }
    },
    {
      "name": "diff: changed",
      "scope": "markup.changed",
      "settings": {
        "fontStyle": "",
        "foreground": "#FF6347"
      }
    },
    {
      "name": "diff: inserted",
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Markup Quote",
      "scope": "markup.quote",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Markup Lists",
      "scope": "markup.list",
      "settings": {
        "foreground": "#00FFFF"
      }
    },
    {
      "name": "Markup Styling",
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "foreground": "#FF69B4"
      }
    },
    {
      "name": "Markup: Strong",
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup: Emphasis",
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.strikethrough",
      "settings": {
        "fontStyle": "strikethrough"
      }
    },
    {
      "name": "Markup Inline",
      "scope": "markup.inline.raw",
      "settings": {
        "fontStyle": "",
        "foreground": "#00FF7F"
      }
    },
    {
      "name": "Markup Headings",
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#1E90FF"
      }
    },
    {
      "name": "Markup Setext Header",
      "scope": "markup.heading.setext",
      "settings": {
        "fontStyle": "",
        "foreground": "#1E90FF"
      }
    }
  ],
  "colors": {
    "focusBorder": "#00FFFF99",
    "selection.background": "#00FFFF99",
    "input.background": "#0D0D0D",
    "input.foreground": "#C0C0C0",
    "input.placeholderForeground": "#C0C0C099",
    "inputOption.activeBorder": "#00FFFF99",
    "inputValidation.infoBorder": "#2C2C2C",
    "inputValidation.infoBackground": "#0D0D0D",
    "inputValidation.warningBackground": "#4A4A4A",
    "inputValidation.warningBorder": "#666666",
    "inputValidation.errorBackground": "#571b26",
    "inputValidation.errorBorder": "#FF4500",
    "errorForeground": "#FF6347",
    "badge.background": "#1E90FF",
    "progressBar.background": "#1E90FF",
    "dropdown.background": "#0D0D0D",
    "dropdown.border": "#00FFFF99",
    "button.background": "#00FFFF99",
    "list.activeSelectionBackground": "#00FFFF",
    "quickInputList.focusBackground": "#00FFFF",
    "list.hoverBackground": "#004454AA",
    "list.inactiveSelectionBackground": "#00445488",
    "list.dropBackground": "#00445488",
    "list.highlightForeground": "#00FFFF",
    "editor.background": "#0D0D0D",
    "editor.foreground": "#C0C0C0",
    "editorWidget.background": "#0D0D0D",
    "editorWidget.foreground": "#C0C0C0",
    "editorLineNumber.foreground": "#00FFFF",
    "editorCursor.foreground": "#C0C0C0",
    "editorWhitespace.foreground": "#4A4A4A",
    "editor.selectionBackground": "#00FFFF99",
    "editor.selectionForeground": "#C0C0C0",
    "editor.wordHighlightBackground": "#004454AA",
    "editor.wordHighlightBorder": "#00FFFF",
    "editorBracketMatch.background": "#006666",
    "editorBracketMatch.border": "#00FFFF",
    "editorLink.activeForeground": "#00FFFF"
  }
}
